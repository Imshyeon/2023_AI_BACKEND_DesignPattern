\
```python\
from django.shortcuts import render\
from .models import Post\
\
def recent_posts_view(request):\
    # \uc0\u52572 \u44540  \u44172 \u49884 \u47932 \u51012  \u44032 \u51256 \u50752 \u49436  \u54596 \u53552 \u47553 \
    recent_posts = Post.objects.filter(is_published=True, is_recent=True)\
    \
    # \uc0\u48624  \u47196 \u51649 \u50640 \u49436  \u45796 \u47480  \u51089 \u50629 \u51012  \u49688 \u54665 \u54624  \u49688  \u51080 \u51020 \
    \
    # \uc0\u53596 \u54540 \u47551 \u50640  \u45936 \u51060 \u53552  \u51204 \u45804 \
